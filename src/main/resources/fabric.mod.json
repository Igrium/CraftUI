{
    "schemaVersion": 1,
    "id": "craftui",
    "version": "${version}",
    "name": "CraftUI",
    "description": "An implementation of Dear ImGui on top of Minecraft for in-game editors.",
    "authors": [
        "Igrium"
    ],
    "contact": {
        "homepage": "https://github.com/Igrium/CraftUI",
        "sources": "https://github.com/Igrium/CraftUI"
    },
    "icon": "assets/craftui/icon.png",
    "environment": "client",
    "mixins": [
        "craftui.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14.6",
        "minecraft": "~1.21.4",
        "java": ">=17",
        "fabric-api": "*"
    },
    "suggests": {
    },
    "entrypoints": {
        "client": [
            "com.igrium.craftui.CraftUI"
        ],
        "modmenu": [
            "com.igrium.craftui.CraftUIModMenu"
        ]
    }
}